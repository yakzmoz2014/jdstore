<h2> 修改个人资料 </h2>
<%= simple_form_for [:account, @user] do |f| %>
  <div>
    <%= f.input :username, label: "用户名" %>
    <%= f.input :email, label: "邮箱地址" %>
    <%= link_to("取消", account_users_path, class: "btn btn-default" ) %>
    <%= f.submit "提交", class: "btn  btn-default", data: { disable_with: "Submitting..." } %>
  </div>
<% end %>
